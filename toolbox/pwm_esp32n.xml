<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_pwm_esp32n" style="display: none">
    <category name="%{BKY_TSUMICKY_CATEGORY_PWM_ESP32N}" id="category_pwm_esp32n" colour="#5b80a5">
        <block type="pwm_esp32n_attach">
            <field name="ws_var">sock</field>
            <field name="pin_var">pin</field>
            <value name="freq">
                <shadow type="math_number">
                    <field name="NUM">10000</field>
                </shadow>
            </value>
            <value name="bit">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="pwm_esp32n_write">
            <field name="ws_var">sock</field>
            <field name="pin_var">pin</field>
            <value name="duty">
                <shadow type="math_number">
                    <field name="NUM">50</field>
                </shadow>
            </value>
        </block>
    </category>
</xml>
