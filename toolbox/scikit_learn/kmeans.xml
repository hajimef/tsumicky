<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_sl_kmeans" style="display: none">
  <category id="category_sl_kmeans" name="%{BKY_TSUMICKY_CATEGORY_SL_KMEANS}" colour="#5ba593">
    <block type="sl_kmeans_init">
      <value name="num">
        <shadow type="math_number">
          <field name="NUM">2</field>
        </shadow>
      </value>
      <value name="random_state">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="sl_kmeans_fit">
    </block>
    <block type="sl_kmeans_plot">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="x">
        <block type="sl_data_slice_number">
          <value name="num">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
        </block>
      </value>
      <value name="y">
        <block type="sl_data_slice_number">
          <value name="num">
            <shadow type="math_number">
              <field name="NUM">2</field>
            </shadow>
          </value>
        </block>
      </value>
      <value name="p_marker">
        <block type="mpl_chart_markers">
          <field name="markers">o</field>
        </block>
      </value>
      <value name="p_size">
        <block type="math_number">
          <field name="NUM">5</field>
        </block>
      </value>
      <value name="c_marker">
        <block type="mpl_chart_markers">
          <field name="markers">*</field>
        </block>
      </value>
      <value name="c_color">
        <block type="colour_picker">
          <field name="COLOUR">#ff0000</field>
        </block>
      </value>
      <value name="c_size">
        <block type="math_number">
          <field name="NUM">10</field>
        </block>
      </value>
    </block>
    <block type="sl_data_slice_number">
      <value name="num">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="sl_data_slice_label">
      <value name="label">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="sl_kmeans_label">
      <value name="range">
        <block type="excel_range">
          <value name="sheet">
            <block type="excel_active_sheet">
              <value name="workbook">
                <block type="excel_active_workbook"></block>
              </value>
            </block>
          </value>
          <value name="range">
            <shadow type="text">
              <field name="TEXT">A1</field>
            </shadow>
          </value>
        </block>
      </value>
    </block>
    <block type="sl_kmeans_center">
      <value name="range">
        <block type="excel_range">
          <value name="sheet">
            <block type="excel_active_sheet">
              <value name="workbook">
                <block type="excel_active_workbook"></block>
              </value>
            </block>
          </value>
          <value name="range">
            <shadow type="text">
              <field name="TEXT">A1</field>
            </shadow>
          </value>
        </block>
      </value>
    </block>
  </category>
</xml>