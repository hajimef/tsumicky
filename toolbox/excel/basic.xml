<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_excel_basic" style="display: none">
    <category name="%{BKY_TSUMICKY_CATEGORY_EXCEL_BASIC}" id="category_excel_basic" colour="#5ba55b">
        <block type="excel_launch">
            <field name="visible">1</field>
            <value name="host">
                <shadow type="text">
                    <field name="TEXT">localhost</field>
                </shadow>
            </value>
            <value name="port">
                <shadow type="math_number">
                    <field name="NUM">8001</field>
                </shadow>
            </value>
        </block>
        <block type="excel_quit">
        </block>
        <block type="excel_new_file">
        </block>
        <block type="excel_find_file">
        </block>
        <block type="excel_open_file">
            <value name="filename">
                <shadow type="text">
                    <field name="TEXT"></field>
                </shadow>
            </value>
        </block>
        <block type="excel_save_as_file">
            <value name="filename">
                <shadow type="text">
                    <field name="TEXT"></field>
                </shadow>
            </value>
        </block>
        <block type="excel_save_file">
        </block>
        <block type="excel_screen_updating">
        </block>
        <block type="excel_os_special_folder_path">
            <field name="folder">0</field>
        </block>
        <block type="excel_os_join_path">
            <mutation items="2"></mutation>
        </block>
        <block type="excel_basic_cells_set">
            <value name="row">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="col">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="excel_basic_cells_formula">
            <value name="row">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="col">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="formula">
                <shadow type="text">
                    <field name="TEXT">=</field>
                </shadow>
            </value>
        </block>
        <block type="excel_basic_cells_get">
            <value name="row">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="col">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="excel_run_book_macro">
            <field name="macro">macro</field>
            <value name="params">
                <block type="lists_create_with" deletable="false" movable="false">
                    <mutation items="1"></mutation>
                </block>
            </value>
        </block>
    </category>
</xml>
