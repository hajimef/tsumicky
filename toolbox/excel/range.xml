<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_excel_range" style="display: none">
  <category name="%{BKY_TSUMICKY_CATEGORY_EXCEL_RANGE}" id="category_excel_range" colour="#5b67a5">
    <block type="excel_cell_rc">
      <value name="sheet">
        <block type="excel_active_sheet">
          <value name="workbook">
            <block type="excel_active_workbook"></block>
          </value>
        </block>
      </value>
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="excel_range">
      <value name="sheet">
        <block type="excel_active_sheet">
          <value name="workbook">
            <block type="excel_active_workbook"></block>
          </value>
        </block>
      </value>
      <value name="range">
        <shadow type="text">
          <field name="TEXT">A1</field>
        </shadow>
      </value>
    </block>
    <block type="excel_set_range_do">
      <value name="range">
        <block type="excel_cell_rc">
          <value name="sheet">
            <block type="excel_active_sheet">
              <value name="workbook">
                <block type="excel_active_workbook"></block>
              </value>
            </block>
          </value>
          <value name="row">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
          <value name="col">
            <shadow type="math_number">
              <field name="NUM">1</field>
          </shadow>
          </value>
        </block>
      </value>
    </block>
    <block type="excel_current_cell"></block>
    <block type="excel_set_cell_value">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
    </block>
    <block type="excel_get_cell_value">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
    </block>
    <block type="excel_set_cell_formula">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="formula">
        <shadow type="text">
          <field name="TEXT">=</field>
        </shadow>
      </value>
    </block>
    <block type="excel_set_cell_number_format">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="format">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="excel_set_cell_font_color">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="color">
        <block type="colour_picker"></block>
      </value>
    </block>
    <block type="excel_set_cell_interior_color">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="color">
        <block type="colour_picker"></block>
      </value>
    </block>
    <block type="excel_set_cell_font_name">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="fontname">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="excel_set_cell_font_size">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="fontsize">
        <shadow type="math_number">
          <field name="NUM">10.5</field>
        </shadow>
      </value>
    </block>
    <block type="excel_set_cell_font_bold">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="is_bold">
        <block type="logic_boolean">
          <field name="BOOL">TRUE</field>
        </block>
      </value>
    </block>
    <block type="excel_set_cell_font_italic">
      <value name="range">
        <block type="excel_current_cell">
          <field name="BOOL">TRUE</field>
        </block>
      </value>
      <value name="is_italic">
        <block type="logic_boolean"></block>
      </value>
    </block>
    <block type="excel_set_cell_border_type">
      <value name="range">
        <block type="excel_current_cell">
          <field name="BOOL">TRUE</field>
        </block>
      </value>
    </block>
    <block type="excel_set_cell_border_color">
      <value name="range">
        <block type="excel_current_cell"></block>
      </value>
      <value name="color">
        <block type="colour_picker"></block>
      </value>

    </block>
  </category>
</xml>