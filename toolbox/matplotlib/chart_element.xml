      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <field name="xy">0</field>
      <value name="ticks">
        <block type="mpl_tick_list">
          <value name="from">
            <shadow type="math_number">
              <field name="NUM">0</field>
            </shadow>
          </value>
          <value name="to">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
          <value name="step">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
        </block>
      </value>
    </block>
    <block type="mpl_chartelm_tick_labels">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <field name="xy">0</field>
      <value name="ticks">
        <block type="mpl_tick_list">
          <value name="from">
            <shadow type="math_number">
              <field name="NUM">0</field>
            </shadow>
          </value>
          <value name="to">
            <shadow type="math_number">
              <field name="NUM">10</field>
            </shadow>
          </value>
          <value name="step">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
        </block>
      </value>
      <value name="labels">
        <block type="lists_create_with"></block>
      </value>
    </block>
    <block type="mpl_chartelm_lim">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <field name="xy">0</field>
      <value name="from">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="to">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="mpl_chartelm_grid">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="linewidth">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <field name="linestyle">-</field>
      <field name="xy">0</field>
      <value name="color">
        <block type="colour_picker">
          <field name="COLOUR">#000000</field>
        </block>
      </value>
    </block>
