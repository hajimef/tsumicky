<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_mpl_chart_element" style="display: none">
  <category name="%{BKY_TSUMICKY_CATEGORY_MPL_CHART_ELEMENT}" id="category_mpl_chart_element" colour="#5ba58c">
    <block type="mpl_chartelm_figure_title">
      <value name="title">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="mpl_chartelm_title">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="title">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="mpl_chartelm_legend">
      <value name="row">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="col">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="ncol">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <field name="loc">best</field>
      <value name="title">
        <shadow type="text">
          <field name="TEXT"></field>
        </shadow>
      </value>
    </block>
    <block type="mpl_chartelm_axis_label">
      <value name="row">
        <shadow type="math_number">
