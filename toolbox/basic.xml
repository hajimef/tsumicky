<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox_basic" style="display: none">
    <category name="%{BKY_TSUMICKY_CATEGORY_BASIC}" id="category_basic" colour="#a5745b">
        <block type="basic_console_log">
            <value name="text">
                <shadow type="text">
                    <field name="TEXT">Hello</field>
                </shadow>
            </value>
        </block>
        <block type="basic_format_float">
            <value name="num">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <field name="all">0</field>
            <field name="u0">0</field>
        </block>
        <block type="basic_sleep">
            <value name="ms">
                <shadow type="math_number">
                    <field name="NUM">1000</field>
                </shadow>
            </value>
        </block>
        <block type="basic_stop">
        </block>
    </category>
</xml>

